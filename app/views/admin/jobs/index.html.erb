<div class = "pull-right">
  <%= link_to("新增", new_admin_job_path, :class => "btn btn-default")%>
</div>
<br><br>

<table class= "table table-boldered">
  <% @jobs.each do |job| %>
  <tr>
    <td>
      <%= link_to(job.title, admin_job_path(job))%>
    </td>
    <td>
      <%= link_to("编辑",edit_admin_job_path(job))%>
      <%= link_to("删除",admin_job_path(job), :method => :delete,
        :data => {:confirm => "确定删除吗？"})%>
    </td>
    <td>
      <%= job.created_at %>
    </td>
  </tr>
  <% end %>
</table>
